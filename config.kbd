(defcfg
    input    (device-file "/dev/input/by-id/usb-YOUR_USB_DEVICE-event-kbd")
    output   (uinput-sink "My KMonad output")
    fallthrough true
)

(defsrc
             1        2        3        4        5        6        7        8        9        0
    tab      q        w        e        r        t        y        u        i        o        p        [	]
    caps     a        s        d        f        g        h        j        k        l        ;        '        ret
    lsft     z        x        c        v        b        n        m        ,        .        /        rsft
    lctl                                                  spc               ralt
)

(defalias
